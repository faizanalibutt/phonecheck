<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_spen"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@android:color/white"
    tools:context="com.upgenicsint.phonecheck.activities.SPenActivity">


    <include layout="@layout/navi_buttons"
        android:layout_height="wrap_content"
        android:layout_width="0dp"
        custom:layout_constraintRight_toRightOf="parent"
        custom:layout_constraintLeft_toLeftOf="parent"
        custom:layout_constraintTop_toTopOf="parent"
        android:id="@+id/include" />

    <FrameLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        custom:layout_constraintRight_toRightOf="parent"
        custom:layout_constraintLeft_toLeftOf="parent"
        custom:layout_constraintBottom_toBottomOf="parent"
        android:id="@+id/container"
        custom:layout_constraintHorizontal_bias="0.0"
        custom:layout_constraintVertical_bias="0.0"
        custom:layout_constraintTop_toTopOf="@+id/include">

    </FrameLayout>
</android.support.constraint.ConstraintLayout>
